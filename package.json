{
  "requires": true,
  "lockfileVersion": 1,
  "scripts": {
    "sample": "node ./node_modules/cucumber/bin/cucumber-js features/ -f json:reports/cucumber_report.json --tags '(@Course and @Assessment) and (not @wip)'",
    "run": "node ./node_modules/cucumber/bin/cucumber-js features/ -f json:reports/cucumber_report.json --tags '(@Course and @Assessment) and (not @wip)'",
    "tinder": "node ./node_modules/cucumber/bin/cucumber-js features/Tinder/tests/Likes.feature -f json:reports/cucumber_report.json"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "axe-webdriverjs": "latest",
    "chai": "latest",
    "chai-sorted": "latest",
    "csvtojson": "latest",
    "cucumber": "^6.0.5",
    "cucumber-html-reporter": "latest",
    "faker": "latest",
    "imap-simple": "latest",
    "jsonwebtoken": "latest",
    "mailparser": "latest",
    "nodemailer": "latest",
    "selenium-webdriver": "latest",
    "test-automation-pack": "^0.0.21",
    "minimist": "latest",
    "jsonfile": "latest"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "latest",
    "eslint-plugin-import": "^2.20.2",
    "gherkin-lint": "^4.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.5",
    "prettier": "^2.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,sh,groovy}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.feature": "gherkin-lint -c .gherkin-lintrc.json"
  }
}
