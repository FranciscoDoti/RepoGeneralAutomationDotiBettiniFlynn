{
  "requires": true,
  "lockfileVersion": 1,
  "scripts": {
    "sample": "node ./node_modules/cucumber/bin/cucumber-js features/ -f json:reports/cucumber_report.json --tags '(@Course and @Assessment) and (not @wip)'",
    "run": "node ./node_modules/cucumber/bin/cucumber-js features/ -f json:reports/cucumber_report.json --tags '(@Course and @Assessment) and (not @wip)'",
    "test": "node ./node_modules/cucumber/bin/cucumber-js features/ASSESSMENT/tests/sac/sac-grading-matrix-hw-policy.feature -f json:reports/cucumber_report.json"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "axe-webdriverjs": "latest",
    "chai": "latest",
    "chai-sorted": "latest",
    "chromedriver": "latest",
    "csvtojson": "latest",
    "cucumber": "^6.0.5",
    "cucumber-html-reporter": "latest",
    "dateformat": "latest",
    "faker": "latest",
    "geckodriver": "latest",
    "imagemin": "^7.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imap": "latest",
    "imap-simple": "latest",
    "js-datepicker": "^4.6.1",
    "jsonfile": "latest",
    "jsonwebtoken": "latest",
    "mailparser": "latest",
    "minimist": "latest",
    "nodemailer": "latest",
    "parse-duration": "latest",
    "request-promise-native": "latest",
    "selenium-webdriver": "latest",
    "testrail-api": "latest",
    "winston": "latest"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "latest",
    "eslint-plugin-import": "^2.20.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.5",
    "prettier": "^2.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,sh,groovy,feature}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
